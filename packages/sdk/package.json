    "@near-js/signers": "^2.1.0",
    "@near-js/tokens": "^2.1.0",
    "@near-js/transactions": "^2.1.0",
    "@near-js/utils": "^2.1.0",
    "@near-js/types":"2.1.0"
    
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@changesets/cli": "^2.27.9",
    "@types/bn.js": "^5.1.6",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "ts-node": "^10.9.2",
    "typescript": "catalog:",
    "vitest": "^2.1.3"
  },
  "peerDependencies": {
    "@mysten/sui": "^1.14.1",
    "@mysten/wallet-standard": "^0.13.16",
    "bignumber.js": "9.1.2",
    "icon-sdk-js": "catalog:",
    "rlp": "3.0.0",
    "viem": "catalog:"
  }
