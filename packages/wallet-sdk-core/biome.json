{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "extends": ["../../biome.json"],
  "files": {},
  "linter": {
    "rules": {
      "style": {
        "noUselessElse": "off",
        "noNonNullAssertion": "off"
      },
      "suspicious": {
        "noExplicitAny": "off",
        "noFallthroughSwitchClause": "error"
      },
      "complexity": {
        "noStaticOnlyClass": "off"
      },
      "correctness": {
        "noUnusedFunctionParameters": "off"
      }
    },
    "ignore": []
  }
}
