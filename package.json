{
  "name": "sodax-frontend",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.8.0",
  "scripts": {
    "build": "turbo run build",
    "build:packages": "turbo run build:packages",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:demo": "turbo run dev --filter=demo",
    "pretty": "turbo run pretty",
    "lint": "turbo run lint",
    "checkTs": "turbo run checkTs",
    "test": "turbo run test",
    "clean": "turbo clean && rm -rf node_modules dist .turbo && rm -rf apps/*/node_modules apps/*/dist apps/*/.turbo && rm -rf packages/*/node_modules packages/*/dist packages/*/.turbo",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "turbo": "^2.4.0"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  }
}
