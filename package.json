{
  "name": "sodax-frontend",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.8.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:solver": "turbo dev --filter=react-solver-example",
    "pretty": "turbo pretty",
    "lint": "turbo lint",
    "checkTs": "turbo checkTs",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules dist .turbo && rm -rf apps/*/node_modules apps/*/dist apps/*/.turbo && rm -rf packages/*/node_modules packages/*/dist packages/*/.turbo",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "turbo": "^2.4.0"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "dependencies": {
    "react-dom": "catalog:",
    "react": "catalog:"
  }
}
